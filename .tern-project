{
  "ecmaVersion": "7",
  "libs": [],
  "loadEagerly": [
    "server/*.js"
  ],
  "dontLoad": [
    "node_module/**",
    "client/*"
  ],
  "plugins": {
    "doc_comment": true,
    "complete_strings": {
      "maxLength": 15
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "node-express": {}
  }
}
